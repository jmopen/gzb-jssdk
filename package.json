{
  "name": "@gdjiami/gzb-jssdk",
  "version": "1.10.2",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "description": "MyGZB Webview JSSDK",
  "main": "dist/gzb-jssdk.js",
  "umd:main": "dist/gzb-jssdk.umd.js",
  "module": "dist/gzb-jssdk.module.js",
  "typings": "dist/index.d.ts",
  "author": "ivan",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:ts && npm run build:copy && rollup --config rollup.config.js",
    "build:ts": "rimraf lib && tsc",
    "build:doc": "rimraf docs/api && typedoc --out ./docs/api --theme markdown --readme ./README.md --ignoreCompilerErrors --exclude \"**/__tests__/*\" ./src",
    "build:copy": "cpr lib dist --delete-first --filter \"\\.js\" # copy Typescript declarations to dist",
    "postbuild": "cp dist/gzb-jssdk.umd.js customApi.js",
    "watch": "npm run build:ts && concurrently --kill-others  \"tsc --watch\" \"rollup --config rollup.config.js --watch\"",
    "clean": "rimraf lib dist docs/api",
    "serve": "http-server ./ -g -o",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint -c tslint.json \"src/**/*.ts?(x)\"",
    "prettier": "prettier --single-quote --trailing-comma all --no-semi --write \"src/**/*.ts?(x)\"",
    "precommit": "lint-staged",
    "presetup": "npm i shelljs && npm init",
    "deploy:docs": "gh-pages -d docs/api"
  },
  "lint-staged": {
    "*.ts?(x)": ["npm run prettier", "git add"]
  },
  "keywords": ["typescript"],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json"]
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "concurrently": "^3.5.0",
    "cpr": "^2.2.0",
    "eruda": "^1.2.6",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "jest": "^21.0.1",
    "jh-tslint": "^1.0.0",
    "lint-staged": "^4.0.4",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "rollup": "^0.48.2",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "shelljs": "^0.8.5",
    "ts-jest": "^21.0.0",
    "tslint": "^5.6.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typedoc": "^0.8.0",
    "typedoc-plugin-external-module-name": "^1.0.9",
    "typedoc-plugin-markdown": "^1.0.8",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "http-server": "^0.11.1",
    "tslib": "^1.9.0"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmopen/gzb-jssdk.git"
  },
  "bugs": {
    "url": "https://github.com/jmopen/gzb-jssdk/issues"
  },
  "homepage": "https://github.com/jmopen/gzb-jssdk#readme"
}
